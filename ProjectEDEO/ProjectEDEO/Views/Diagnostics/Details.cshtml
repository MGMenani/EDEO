@model Project_EDEO.Models.Diagnostic

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
    <div class="card-header">
        <strong>Basic Form</strong> Elements
    </div>
    <div class="card-body card-block">
        <form action="" method="post" enctype="multipart/form-data" class="form-horizontal">
            <div class="row form-group">
                <div class="col col-md-3"><label class=" form-control-label">Estimated Age</label></div>
                @Html.DisplayFor(model => model.EstimatedAge)
            </div>
            <div class="row form-group">
                <div class="col col-md-3"><label class=" form-control-label">Image</label></div>
                @Html.DisplayFor(model => model.Image)
            </div>
            <div class="row form-group">
                <div class="col col-md-3"><label class=" form-control-label">ID</label></div>
                @Html.DisplayFor(model => model.MedicalRecordID)
            </div>
            <div class="row form-group">
                <div class="col col-md-3"><label class=" form-control-label">Date</label></div>
                @Html.DisplayFor(model => model.Date)
            </div>
        </form>
    </div>
</div>

 
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.DiagnosticID }) |
    @Html.ActionLink("Back to previous page", null, null, null, new { href = Request.UrlReferrer })
    @Html.ActionLink("Share", null, null, null, new { href = Request.UrlReferrer })
</p>
